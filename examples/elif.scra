(declare
	(name "Elif Example")
)
(define
	(variable i)
)
(when flag
	(say "Like Python!")
	(set i 1)
	(wait 2)
	(repeat_until (> (variable i) 5)
		(change i 1)
		(if (= (variable i) 1)
			(say "First")
		)
		(elif (= (variable i) 2)
			(say "Second")
		)
		(elif (= (variable i) 3)
			(say "Third")
		)
		(elif (= (variable i) 4)
			(say "Fourth")
		)
		(elif (= (variable i) 5)
			(say "Fifth")
		)
		(else
			(say "I never say this")
		)
		(change i 1)
		(wait 2)
	)
)
